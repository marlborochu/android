<resources>

    <style name="AppBaseTheme" parent="@android:style/Theme.Holo.Light.NoActionBar">
        
        
    </style>


    <style name="StyledDialog" parent="@android:style/Theme.Light">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:alertDialogStyle">@style/CustomAlertDialogStyle</item>
        <item name="android:windowBackground">@color/alphablack</item>
    </style>

    <style name="CustomAlertDialogStyle">
        <item name="android:bottomBright">@color/alphablack</item>
        <item name="android:bottomDark">@color/alphablack</item>
        <item name="android:bottomMedium">@color/alphablack</item>
        <item name="android:centerBright">@color/alphablack</item>
        <item name="android:centerDark">@color/alphablack</item>
        <item name="android:centerMedium">@color/alphablack</item>
        <item name="android:fullBright">@color/alphablack</item>
        <item name="android:fullDark">@color/alphablack</item>
        <item name="android:topBright">@color/alphablack</item>
        <item name="android:topDark">@color/alphablack</item>
    </style>

    <style name="leftBehindMenuScroll">
        <item name="android:layout_width">240dp</item>
        <item name="android:layout_height">fill_parent</item>
    </style>

</resources>